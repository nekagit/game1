#:kivy 1.0.9

<Player>:
    size: 60, 80
    canvas.before:
        Rectangle:
            pos: self.pos
            size: self.size
            source: self.source


<LevelTempel>:
    player: player
    canvas:
        Rectangle:
            size: self.size
            source: 'img/background.png'

    Player:
        id: player


<Game>:
    level: level
    canvas.before: 
        Rectangle:
            pos: self.pos
            size: self.size
    LevelTempel:
        id: level